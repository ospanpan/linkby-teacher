.gradeBody {
  display: flex;
  flex-direction: column;
  padding: 20rpx;
  box-sizing: border-box;
}

.import-QRcode {
  height: 600prx;
  padding-top: 200rpx;
  box-sizing: border-box;
}

.import-QRcodeImg {
  width: 320rpx;
  height: 320rpx;
  background-color: #1abc9c;
}

.import-info {
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  font-size: 26rpx;
}
.import-parent,.import-homePage{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  border: 1px solid #1abc9c;
  border-radius: 15rpx;
  margin-top: 30rpx;
  font-size: 30rpx;
}
.import-parent{
  color: #ffffff;
  background-color: #1abc9c
}
.import-homePage{
  color: #1abc9c;
}