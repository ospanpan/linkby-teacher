.notice-list {
  width: 100%;
  display: flex;
  justify-content: space-between;
  overflow: hidden;
}

.del {
  width: 110rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  -webkit-transform: translateX(110rpx);
  transform: translateX(110rpx);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  height: 120rpx;
}

.del image {
  height: 80rpx;
  width: 80rpx;
}

.touch-move-active .notice-item, .touch-move-active .del {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

.notice-item {
  padding: 10rpx 20rpx;
  display: flex;
  width: 100%;
  height: 120rpx;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(110rpx);
  transform: translateX(110rpx);
  margin-left: -110rpx;
}

.notice-sign {
  width: 50rpx;
